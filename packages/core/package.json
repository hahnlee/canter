{
  "name": "@canter/core",
  "version": "0.0.0",
  "napi": {
    "name": "core",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "dependencies": {
    "@node-rs/helper": "^1.2.1"
  },
  "devDependencies": {
    "@napi-rs/cli": "^1.2.1"
  },
  "engines": {
    "node": ">= 10"
  },
  "os": [
    "darwin"
  ],
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version"
  },
  "optionalDependencies": {
    "@canter/core-darwin-arm64": "0.0.0",
    "@canter/core-darwin-x64": "0.0.0"
  }
